{
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "checkMoreOptionsHere": "https://prettier.io/docs/en/options.html",
    "endOfLine": "lf",
    "eslintIntegration": true,
    "htmlWhitespaceSensitivity": "strict",
    "jsxSingleQuote": true,
    "options": {
        "editorconfig": true
    },
    "overrides": [],
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false
}
