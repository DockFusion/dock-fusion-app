{
    "name": "dock-fusion",
    "version": "1.0.0",
    "type": "commonjs",
    "description": "DockFusion is powerful and high performant local development studio unlike any other",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "electron": "npm run electron-build && electron ./dist/electron/electron/main.js",
        "electron-build": "tsc -p ./electron/tsconfig.json",
        "electron-watch": "nodemon --exec \"npm run electron\"",
        "up": "concurrently \"npm run start\" \"npm run electron-watch\""
    },
    "keywords": [
        "DockFusion"
    ],
    "author": "DockFusion Contributors",
    "license": "GNU Affero General Public License v3.0",
