        "@types/dns2": "^2.0.9",
        "@types/dockerode": "^3.3.32",
        "@types/js-yaml": "^4.0.9",
        "@types/lodash": "^4.17.13",
        "@types/node": "^22.10.0",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/semver": "^7.5.8",
